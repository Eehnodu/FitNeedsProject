  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- PurchaseMapper.xml -->
<mapper namespace="com.test.mapper.PurchaseMapper">
	<insert id="insert_Purchase" parameterType="com.test.model.PurchaseVO">
